{
  "name": "blotify-fe",
  "version": "1.0.0",
  "description": "Personal music streamer",
  "main": "src/index.js",
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>src/setupTests.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "\\.(css)$": "identity-obj-proxy",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js"
    }
  },
  "scripts": {
    "test": "jest --verbose",
    "test:watch": "npm run test -- --watch",
    "start": "webpack-dev-server --hot --mode development --devtool eval-source-map",
    "build": "webpack -p --devtool source-map"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ianandrewson/blotify-fe.git"
  },
  "author": "Ian Andrewson",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ianandrewson/blotify-fe/issues"
  },
  "homepage": "https://github.com/ianandrewson/blotify-fe#readme"
}
